<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
        "http://castor.org/mapping.dtd">
<mapping>
    <description>
        Spring OXM and Castor
    </description>
    <class name="com.tabula.drugs.dto.IdGroup">
        <map-to xml="idGroup"/>
        <field name="rxnormId" type="string">
            <bind-xml name="rxnormId" node="element"/>
        </field>
        <field name="name" type="string">
            <bind-xml name="name" node="element"/>
        </field>
    </class>

    <class name=" com.tabula.drugs.dto.RxNormDataDto">
        <map-to xml="rxnormdata"/>
        <field name="idGroup" type="com.tabula.drugs.dto.IdGroup"/>
    </class>
</mapping>